<VirtualHost *:80>
    ServerName example.localhost

    DocumentRoot "/code/example-site/public"

    ProxyPassMatch ^/(.*\.php(/.*)?)$ fcgi://php71:9000/code/example-site/public/$1

    <Directory /code/example-site/public>
        Options +FollowSymLinks +Includes +MultiViews -Indexes
        AllowOverride all
        Require all granted
    </Directory>
</VirtualHost>
